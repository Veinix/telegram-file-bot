{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "*"
            ]
        },
        "moduleResolution": "Node",
        "outDir": "dist",
        "importsNotUsedAsValues": "remove",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "node_modules"
    ]
}